<profile>

    <keyBindings>

<!-- Tags are only key. ID is the identification name, code is the relevant SDL KeyCode -->
        <key id="up" code="119" />
        <key id="down" code="115" />
        <key id="left" code="97" />
        <key id="right" code="100" />

    </keyBindings>

    <options>
        
<!-- Only tag is set -> It will set an option to a value -->
        <set option="NLsize" value="50" />

    </options>

</profile>
